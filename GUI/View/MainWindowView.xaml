<Window x:Class="TicTacToe.View.MainWindowView"
        x:Name="MainScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Home Screen" Height="450" Width="800">
    <Grid>
        <Button Content="TicTacToe" HorizontalAlignment="Left" Margin="191,139,0,0" VerticalAlignment="Top" Width="75" Command="{Binding Source={StaticResource Locator}, Path=MainView.GoToTicTacToeCommand}" CommandParameter="{Binding ElementName=MainScreen}"/>
    </Grid>
</Window>
